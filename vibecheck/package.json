{
  "name": "vibecheck",
  "private": true,
  "workspaces": [
    "packages/scoring",
    "packages/probe",
    "packages/web"
  ],
  "scripts": {
    "build:scoring": "npm run build -w packages/scoring",
    "build:probe": "npm run build -w packages/probe",
    "build:web": "npm run build -w packages/web",
    "build": "npm run build:scoring && npm run build:probe && npm run build:web",
    "dev:web": "npm run dev -w packages/web"
  }
}
